{
    "manifest_version": 2,
    "name": "Cyber Doggo",
    "background": {
        "scripts": ["/JS/background.js"],
        "persistent": false
    },
    "description": "This extension is designed to win some money",
    "version": "1.0",
    "browser_action": {
        "default_icon": "Images/Doodler.png",
        "default_popup": "HTML/Dashboard.html"
    },
    "permissions": ["activeTab", "tabs", "webNavigation"],
    "content_security_policy": "script-src 'self' https://unpkg.com/@google/model-viewer/dist/model-viewer.min.js https://www.gstatic.com/firebasejs/9.14.0/firebase-app.js https://www.gstatic.com/firebasejs/9.14.0/firebase-auth.js https://www.gstatic.com/firebasejs/9.14.0/firebase-database.js https://www.googleapis.com/ https://*.firebaseio.com; object-src 'self'"

}